{
  "compilerOptions": {
    "target": "es5",
    "module": "es6",
    "sourceMap": true,
    "moduleResolution": "node",
    "lib": ["dom", "es2016"],
    "jsx": "react",
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "paths": {
      "app/*": ["./src/*"]
    }
  }
}
